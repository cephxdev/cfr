package dev.cephx.cfr.source;

import java.util.Base64;
import java.util.Map;

import static java.util.Map.entry;

interface VMConstants {
{% for entry in classes %}
    byte[] {{ entry.key | upper | replace({'.': '_'}) }} = decode("{{ entry.value }}");
{% endfor %}

    Map<String, byte[]> CLASSES = Map.ofEntries(
{% for entry in classes %}
        entry("{{ entry.key | replace({'.': '/'}) }}", {{ entry.key | upper | replace({'.': '_'}) }}){% if loop.last %}{% else %},{% endif %}

{% endfor %}
    );

    private static byte[] decode(String s) {
        return Base64.getDecoder().decode(s);
    }
}
